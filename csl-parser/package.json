{
  "name": "csl-parser",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "test": "node --import tsx --test 'main/**/test/*.test.ts' 'shared/**/test/*.test.ts'",
    "test:watch": "node --import tsx --test --watch 'main/**/test/*.test.ts'",
    "test:coverage": "node --import tsx --test --experimental-test-coverage 'main/**/test/*.test.ts'",
    "test:debug": "node --import tsx --inspect-brk --test 'main/**/test/*.test.ts'",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.5.0"
  }
}